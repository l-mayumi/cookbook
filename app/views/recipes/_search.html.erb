<%= form_with(url: "/search", method: "get", local: true) do %>
  <div class="form-inline">
    <div class="form-group mx-auto">
      <span class="mr-sm-1"><%= text_field_tag :busca, nil, class: "form-control" %></span>
      <%= submit_tag 'Buscar', class: "btn btn-dark btn-sm" %>
    </div>
  </div>
<% end %>